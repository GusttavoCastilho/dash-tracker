{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"],
      "@data/*": ["src/data/*"],
      "@domain/*": ["src/domain/*"],
      "@infra/*": ["src/infra/*"],
      "@main/*": ["src/main/*"],
      "@presentation/*": ["src/presentation/*"],
      "@tests/*": ["tests/*"]
    },
    "typeRoots": ["src/presentation/@types/env"]
  },
  "extends": "expo/tsconfig.base"
}
